{
  "name": "expo-changelog-viewer",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md,html}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md,html}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "ci": "yarn format && yarn lint && yarn type-check && yarn test:coverage",
    "type-check": "tsc --noEmit",
    "prepare": "lefthook install"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "1.1.15",
    "@radix-ui/react-checkbox": "1.3.3",
    "@radix-ui/react-collapsible": "1.1.12",
    "@radix-ui/react-dialog": "1.1.15",
    "@radix-ui/react-popover": "1.1.15",
    "@radix-ui/react-progress": "1.1.7",
    "@radix-ui/react-select": "2.2.6",
    "@radix-ui/react-tooltip": "1.2.8",
    "@tailwindcss/typography": "0.5.19",
    "@tailwindcss/vite": "4.1.15",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "dompurify": "3.3.0",
    "lucide-react": "0.545.0",
    "react": "19.2.0",
    "react-complex-tree": "2.6.1",
    "react-dom": "19.2.0",
    "react-markdown": "10.1.0",
    "rehype-raw": "7.0.0",
    "rehype-sanitize": "6.0.0",
    "remark-gfm": "4.0.1",
    "sonner": "2.0.7",
    "tailwind-merge": "3.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@eslint/js": "9.38.0",
    "@ianvs/prettier-plugin-sort-imports": "4.7.0",
    "@mswjs/data": "0.16.2",
    "@playwright/test": "1.56.1",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/dompurify": "3.2.0",
    "@types/jsdom": "27",
    "@types/node": "24.9.1",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@vitejs/plugin-react": "5.0.4",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "eslint": "9.38.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.0",
    "eslint-plugin-react-refresh": "0.4.23",
    "eslint-plugin-typescript-sort-keys": "3.3.0",
    "fake-indexeddb": "6.2.4",
    "gh-pages": "6.3.0",
    "happy-dom": "20.0.7",
    "jsdom": "27.0.1",
    "lefthook": "1.13.6",
    "msw": "2.11.6",
    "playwright": "1.56.1",
    "prettier": "3.6.2",
    "tailwindcss": "4.1.15",
    "terser": "5.44.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.2",
    "vite": "7.1.11",
    "vitest": "3.2.4"
  },
  "packageManager": "yarn@4.9.4"
}
