{
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react$",
    "",
    "<THIRD_PARTY_MODULES>",
    "",
    "^@/contexts/(.*)$",
    "",
    "^@/hooks/(.*)$",
    "",
    "^@/components/(.*)$",
    "",
    "^@/utils/(.*)$",
    "",
    "^@/lib/(.*)$",
    "",
    "^[./]",
    "",
    "<TYPES>",
    "<TYPES>^[.]"
  ],
  "importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"],
  "importOrderTypeScriptVersion": "5.9.3",
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "useTabs": false,
  "trailingComma": "es5",
  "printWidth": 100,
  "bracketSpacing": true,
  "arrowParens": "always",
  "endOfLine": "lf"
}
